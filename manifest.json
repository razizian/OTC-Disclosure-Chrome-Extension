{
    "manifest_version": 2,

    "name": "OTC Disclosure",
    "description": "Quickly access a ticker's SEC filings on OTC with a click of a button",
    "version": "1.0",
    "icons": {
        "16": "image16.png",
        "32": "image32.png",
        "48": "image48.png",
        "128": "image128.png"
    },
    "background": {
        "scripts": ["eventPage.js"]
    },
    "browser_action": {
        "default_icon": "image128.png"
    },
    "permissions": [
        "tabs",
        "contextMenus"
    ]
} 

